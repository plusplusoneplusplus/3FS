{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug meta_main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/meta_main",
            "args": [
                "--launcher_cfg",
                "${workspaceFolder}/testing_configs/minimal_meta_launcher_config.toml",
                "--app_cfg",
                "${workspaceFolder}/testing_configs/minimal_meta_app_config.toml",
                "--cfg",
                "${workspaceFolder}/testing_configs/minimal_meta_server_config.toml"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": true
            }
        },
        {
            "name": "Debug mgmtd_main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/mgmtd_main",
